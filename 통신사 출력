a = list(input("휴대전화 번호 입력: "))

if a[0] == "0" and a[1] == "1" and a[2] == "1":
  print("당신은 SKT 사용자입니다.")

if a[0] == "0" and a[1] == "1" and a[2] == "6":
  print("당신은 KT 사용자입니다.")

if a[0] == "0" and a[1] == "1" and a[2] == "9":
  print("당신은 LGU 사용자입니다.")  

if a[0] == "0" and a[1] == "1" and a[2] == "0":
  print("알수없음")
